; Moodle recommended PHP settings
[PHP]
max_input_vars = 5000
post_max_size = 64M
upload_max_filesize = 64M
max_execution_time = 300
memory_limit = 512M

[opcache]
opcache.enable = 1
opcache.enable_cli = 1
opcache.memory_consumption = 128
opcache.max_accelerated_files = 8000
opcache.revalidate_freq = 1
